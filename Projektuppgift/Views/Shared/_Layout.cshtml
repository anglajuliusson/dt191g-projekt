<!DOCTYPE html>
<html lang="sv">
<head>
    <title>@ViewData["Title"]</title>
    <link rel="stylesheet" href="~/css/style.css" asp-append-version="true" />
</head>
<body>
    <header>
        <h1>Header</h1>
        <nav>
            <ul>
                <li><a asp-controller="Home" asp-action="Index">Startsida</a></li>
                <li><a asp-controller="Home" asp-action="Contact">Kontakt</a></li>
                <li><a asp-controller="Sibling" asp-action="Index">Syskon</a></li>
            </ul>
        </nav>
    </header>
    <main>
        @RenderBody()
    </main>
    <footer>
        <p>Footer</p>
    </footer>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>